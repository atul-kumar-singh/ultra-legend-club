{
  "name": "postcss-utilities",
  "version": "0.8.0",
  "description": "PostCSS plugin to add a collection of mixins, shortcuts, helpers and tools for your CSS",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "utility",
    "library",
    "collection",
    "pack",
    "helpers",
    "mixins",
    "shortcuts",
    "hacks"
  ],
  "author": "Ismael Mart√≠nez <ismael@isma.uy>",
  "license": "MIT",
  "repository": "ismamz/postcss-utilities",
  "bugs": {
    "url": "https://github.com/ismamz/postcss-utilities/issues"
  },
  "homepage": "https://github.com/ismamz/postcss-utilities",
  "dependencies": {
    "postcss": "^6.0.16",
    "postcss-value-parser": "^3.3.0"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "eslint": "^2.13.1",
    "eslint-config-postcss": "^2.0.2",
    "fs": "0.0.2"
  },
  "scripts": {
    "test": "ava && eslint **/*.js"
  },
  "eslintConfig": {
    "extends": "postcss/es5",
